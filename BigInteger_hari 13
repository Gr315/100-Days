package day13;

import java.math.BigInteger;

public class Main {
    
    public static void main(String[] args) {
        BigInteger bigInteger1 = new BigInteger("1234567890");
        BigInteger bigInteger2 = new BigInteger("9876543210");
        
        BigInteger sum = bigInteger1.add(bigInteger2);
        BigInteger difference = bigInteger1.subtract(bigInteger2);
        BigInteger product = bigInteger1.multiply(bigInteger2);
        
        System.out.println("Penjumlahan: " + sum);
        System.out.println("Pengurangan: " + difference);
        System.out.println("Perkalian: " + product);
   
  
    }
    
}
