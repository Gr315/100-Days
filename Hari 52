package pkg100days;

public class day52 {
    
    public static void main(String[] args) {
        int[] dataNilai = {45, 78, 71, 89, 93, 58, 62, 63, 34, 78};
        
        for (int nilai : dataNilai) {
            if (nilai >= 70) {
               System.out.println(nilai + " Lulus");
                
                 } else {
                     System.out.println(nilai + " Tidak Lulus");
                 }
        }
    }
}
