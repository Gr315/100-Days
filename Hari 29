package day29;

import java.util.Scanner;

public class pola {
    
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        
        System.out.print("masukkan baris");
        int a = input.nextInt();
        for(int i = 0; i< a; i++){
            for(int j = 0; j< i; j++){
                System.out.print(" * ");
            }
            System.out.println();
        }
                
                
    }
    
}
