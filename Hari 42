package pkg100day;

public class day42 {
    
    public static void main(String[] args) {
        int n1 = 0, n2 = 1, n3;
        System.out.println(n1 + " " + n2);
        do {
            n3 = n1 + n2;
            if (n3 > 100) {
                break;
            }
            System.out.println(" " + n3);
            
            n1 = n2;
            n2 = n3;
        } while (n1 + n2 <= 100);
    }
    
}
