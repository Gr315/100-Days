package pkg100days;

public class day46;

public static void main (String[] args) {
    int rows = 5;
    int i = rows;
    while (i >= 1) {
        
        int j = 1;
        while (j <= 1) {
            System.out.print("*");
            j++;
            System.out.println();
            i--;
        }
    }
}
}
