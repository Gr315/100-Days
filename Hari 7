package main;

public class day7 {
    
    public static void main(String[] args) {
        byte myByte = 100;
        short myShort = 1000;
        int myInt = 100000;
        long myLong = 10000000000L;
        float myFloat = 10.5f; 
        double myDouble = 10.5;
        char myChar = 'A';
        boolean myBoolean = true;
        
   System.out.println("nilai byte: " + myByte);
   System.out.println("nila short: " + myShort);
   System.out.println("nilai int: " + myInt);
   System.out.println("nilai long: " + myLong);
   System.out.println("nilai float: " + myFloat);
   System.out.println("nilai double: " + myDouble);
   System.out.println("nilai char: " + myChar);
   System.out.println("nilai boolean: " + myBoolean); 
        

        
    }
    
}
