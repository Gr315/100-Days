package pkg100days;

public class day50 {
    
    public static void main(String[] args) {
        boolean[] myArray = new boolean[10];
        for (int i = 0; i < myArray.length i++) {
            if (i % 2 == 0) {
                myArray[i] = true;
            } else {
                myArray[i] = false;
            }
        }
        for (int i = 0; i < myArray.length) {
        System.out.println("indeks" + i + ":" + myArray[i]);
        
        }
    }
}
