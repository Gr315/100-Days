package pkg100days;

public class day48 {
    
    public static void main(String[] args) {
        int[] myArray = new int [5];
        
        myArray[0] = 2;
        myArray[1] = 4;
        myArray[2] = 6;
        myArray[3] = 8;
        myArray[4] = 10;
        
        for (int i = 0; i < myArray.length; i++) {
            System.out.println(myArray[i]);
        }
    }
    
}
